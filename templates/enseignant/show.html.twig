{% extends 'base.html.twig' %}

{% block title %}Enseignant{% endblock %}

{% block body %}
    <h1>Enseignant</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ enseignant.id }}</td>
            </tr>
            <tr>
                <th>Roles</th>
                <td>{{ enseignant.roles ? enseignant.roles|json_encode : '' }}</td>
            </tr>
            <tr>
                <th>Password</th>
                <td>{{ enseignant.password }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ enseignant.nom }}</td>
            </tr>
            <tr>
                <th>Prenom</th>
                <td>{{ enseignant.prenom }}</td>
            </tr>
            <tr>
                <th>Adresse</th>
                <td>{{ enseignant.adresse }}</td>
            </tr>
            <tr>
                <th>Tel</th>
                <td>{{ enseignant.tel }}</td>
            </tr>
            <tr>
                <th>Sexe</th>
                <td>{{ enseignant.sexe }}</td>
            </tr>
            <tr>
                <th>CIN</th>
                <td>{{ enseignant.CIN }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ enseignant.email }}</td>
            </tr>
            <tr>
                <th>Photo</th>
                <td>{{ enseignant.photo }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_enseignant_index') }}">back to list</a>

    <a href="{{ path('app_enseignant_edit', {'id': enseignant.id}) }}">edit</a>

    {{ include('enseignant/_delete_form.html.twig') }}
{% endblock %}
